<table class="table table-striped table-bordered table-condensed">
  <tr>
    <% ebook = Libraryebook.find(relationshp.libraryebook_id)  %>
    <td>Name: <%= ebook.name  %></td>
    <td>Genre: <%= ebook.genre  %></td>
    <td>Deposit Amt: <%= ebook.loanamount  %></td>
    <td>Status: <%= relationshp.status   %></td>
    <td>Period Start: <%= relationshp.loanstart   %></td>
    <td>Period End: <%= relationshp.loanend   %></td>
    <td>Created <%= time_ago_in_words(relationshp.created_at) %> ago.</td>
    <%#= link_to 'Renew', edit_relationshp_path(@relationshp) %>
    <td><%= link_to(relationshp, class: "btn btn-small") do %>
        Show <i class="icon-show icon-white"></i>
    <% end %></td>
  </tr>

</table>